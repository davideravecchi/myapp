<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drave.mioporcino">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MioPorcino">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mycoords" android:screenOrientation="portrait" />
        <activity android:name=".homepage" android:screenOrientation="portrait" />
        <activity android:name=".new_finding" android:screenOrientation="portrait" />
        <activity android:name=".search_findings" android:screenOrientation="portrait" />
        <activity android:name=".findings_management" android:screenOrientation="portrait" />
    </application>

</manifest>